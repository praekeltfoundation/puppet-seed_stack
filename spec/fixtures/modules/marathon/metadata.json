{
  "name": "praekeltfoundation-marathon",
  "version": "0.1.0",
  "author": "Jamie Hewland",
  "summary": "Module that manages the Marathon framework for Mesos",
  "license": "BSD",
  "source": "http://github.com/praekeltfoundation/puppet-marathon",
  "project_page": "https://github.com/praekeltfoundation/puppet-marathon",
